
<div class="container">
	<div class="row-fluid">
		<div class="span8">
			<%= render 'about/about' %>
			<dl class="dl-horizontal">
				<button type ="button" class="btn btn-info" data-toggle="collapse" data-target="#mySkills">
					Skills
				</button>
				<div id="mySkills" class="collapse" >
					<%= render 'about/skills' %>
				</div>
				<button type ="button" class="btn btn-info" data-toggle="collapse" data-target="#myEducation">
					Education
				</button>
				<div id="myEducation" class="collapse" >
					<%= render 'about/education' %>
				</div>
				<button type ="button" class="btn btn-info" data-toggle="collapse" data-target="#myExperience">
					Experience
				</button>
				<div id="myExperience" class="collapse" >
					<%= render 'about/experience' %>
				</div>
			</dl>
		</div>
		<div class="span1">
		</div>
		<div class="span3">
			<%= render 'about/pic' %>
		</div>
	</div>
</div>


